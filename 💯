public class StringCoder{
    private String masterString;
    private StringPart findPart(String str){
        int len=5;
        Stringpart temp=new Stringpart(0,1);
        while(len>=1){
            int a=masterString.indexOf(str.subSring(0,len);
            if(a!=-1){
                return new Stringpart(a,len);
            }
                len--;
        }
        return temp;
    }
}
